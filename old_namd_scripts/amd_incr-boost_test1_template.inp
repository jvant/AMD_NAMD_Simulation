# Equilibrate for 100 ps

#############################################################
## ADJUSTABLE PARAMETERS                                   ##
#############################################################
set myfile         FILENAME

set temp           303.15;
set stepnum        STEPNUM
set outputname     step.$stepnum\_equilibration;
set TS		   1500000


accelMD		   on
accelMDdual	   on
accelMDTE	   -100881
accelMDTalpha	   8374
accelMDFirstStep   0
accelMDLastStep    100000
accelMDOutFreq	   100

source ../../scripts/run.inp